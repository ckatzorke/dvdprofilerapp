<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.1.xsd">

	<bean id="xpathCollectionImporter" class="org.dvdprofilerapp.xml.jaxp.XPathCollectionImporter">
		<property name="collectionXmlResource" value="classpath:test-collection.xml" />
		<property name="dvdProfilerRepository" ref="mockRepo" />
	</bean>
	<bean id="mockRepo" class="org.mockito.Mockito" factory-method="mock">
		<constructor-arg value="org.dvdprofilerapp.DvdProfilerRepository " />
	</bean>


</beans>
